<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for sitemap in sitemaps %}
    <sitemap>
        <loc>{{ _publicUrl }}/{{ sitemap.uri }}</loc>
        <lastmod>{{ sitemap.changed|date('Y-m-d') }}</lastmod>
    </sitemap>
{%  endfor %}
</sitemapindex>
