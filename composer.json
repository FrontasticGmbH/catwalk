{
    "name": "frontastic/catwalk",
    "version": "1.19.4",
    "license": "None",
    "type": "library",
    "repositories": [
        {
            "type": "path",
            "url": "../libraries/*"
        }
    ],
    "config": {
        "platform": {
            "php": "7.4"
        },
        "discard-changes": true
    },
    "require":      {
        "php": ">=7.4",
        "ext-json": "*",

        "symfony/symfony": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/psr-http-message-bridge": "~1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/monolog-bundle": "^3.2",
        "symfony/swiftmailer-bundle": "^3.2",
        "doctrine/doctrine-bundle": "^1.9",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "stof/doctrine-extensions-bundle": "^1.3",

        "frontastic/common": "^2.18.0",

        "doctrine/orm": "^2.6",
        "guzzlehttp/guzzle": "~6",
        "gedmo/doctrine-extensions": "^2.4",

        "sensio/framework-extra-bundle": "@stable",

        "kore/data-object": "@stable",
        "qafoolabs/no-framework-bundle": "@stable",
        "beberlei/doctrineextensions": "@stable",

        "predis/predis": "@stable",
        "michelf/php-markdown": "@stable",
        "aptoma/twig-markdown": "@stable",
        "mtdowling/cron-expression": "@stable",
        "kphoen/rulerz-bundle": "^0.15.0",
        "zetacomponents/graph": "^1.5",
        "ramsey/uuid": "^3.9",
        "jaybizzle/crawler-detect": "^1.2",
        "nikic/php-parser": "^4.12"
    },
    "require-dev":  {
        "frontastic/apidocs": "^1.0.0@alpha",
        "phpunit/phpunit": "^7",
        "phpmd/phpmd": "@stable",
        "pdepend/pdepend": "@stable",
        "phploc/phploc": "@stable",
        "squizlabs/php_codesniffer": "@stable",
        "sebastian/phpcpd": "@stable",
        "phake/phake": "^3.0",
        "phpstan/phpstan": "^0.12.18",
        "mikey179/vfsstream": "^1"
    },
    "autoload": {
        "psr-4": {
            "Frontastic\\Catwalk\\": "src/php",
            "Kameleoon\\": "libraries/kameleoon/kameleoon/client/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Frontastic\\Catwalk\\": "test/php/"
        }
    }
}
